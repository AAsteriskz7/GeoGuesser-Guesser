{
  "manifest_version": 3,
  "name": "Location Guesser",
  "version": "1.0",
  "description": "Takes a screenshot of your webpage and uses the Gemini API to guess the location.",
  "icons": {
    "128": "achueeo.jpg"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "https://generativelanguage.googleapis.com/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "achueeo.jpg"
    }
  }
}
